<div class="container mx-auto mt-6">
    <%= for game <- games @club do %>
        <div class="bg-white mb-4 p-8 rounded-sm border border-grey-light">
            <div>
                <%= game.location %>
            </div>

            <div>
                <%= game.start_at %>
            </div>
        </div>
    <% end %>
</div>